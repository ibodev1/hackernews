{
  "deno.enable": true,
  "deno.cacheOnSave": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },

  /* Disable Linter / Formatter */
  "biome.enabled": false,
  "eslint.enable": false,
  "prettier.enable": false
}
